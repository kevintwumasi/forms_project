public class Data_Tools
{
	// get and set methods
	public Object GetEntry(Struct o) {
		return o.GetEntry();
	}
	public void SetEntry(Struct o, Object Data_Entry) {
		o.SetEntry(Data_Entry);
	}
	// type validation
	public boolean VInt(Struct o) {
		if (o instanceof SInt) {
			return true;
		} else {
			return false;
		}
	}
	public boolean VDouble(Struct o) {
		if (o instanceof SDouble) {
			return true;
		} else {
			return false;
		}
	}
	public boolean VString(Struct o) {
		if (o instanceof SString) {
			return true;
		} else {
			return false;
		}
	}
}
